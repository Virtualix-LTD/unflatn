{
  "name": "unflatn",
  "version": "1.0.5",
  "description": "The opposite of `flatten`",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "postversion": "make-badges",
    "make-badges": "coverage-badges --style flat --source coverage/coverage-summary.json --output doc/coverage-badge.svg",
    "cover": "istanbul cover --report json-summary --print detail test/index.js",
    "view-cover": "istanbul report html && open ./coverage/index.html",
    "dev": "nodemon test | tap-difflet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Virtualix-LTD/unflatn.git"
  },
  "keywords": [
    "flatten",
    "unflat",
    "unflatn",
    "unflatten"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Virtualix-LTD/unflatn/issues"
  },
  "homepage": "https://github.com/Virtualix-LTD/unflatn#readme",
  "contributors": [
    "Tony Klinakis <aklin@users.noreply.github.com>",
    {
      "name": "Timothee Desurmont",
      "url": "https://github.com/Sampaguitas"
    },
    "Jim De Beer <jim@vigour.io>",
    "Shawn Inder <shawn@vigour.io>",
    "Michael Hernandez <michael.hernandez1988@gmail.com>"
  ],
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "engines": {},
  "devDependencies": {
    "coverage-badges-cli": "^2.1.0",
    "istanbul": "^0.4.4",
    "nodemon": "^3.1.10",
    "tap-difflet": "^0.7.2",
    "tape": "^5.9.0"
  },
  "coverageReporters": [
    "json-summary"
  ],
  "files": [
    "lib",
    "doc/coverage-badge.svg",
    "doc/version-badge.svg"
  ]
}
